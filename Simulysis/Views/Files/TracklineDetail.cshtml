@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    @*<meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="~/css/bootstrap.min.css?v=1.0.9.2" rel="stylesheet" type="text/css" />
    <link href="~/css/Site/Site.min.css?v=1.0.9.2" rel="stylesheet" type="text/css" />
    <link href="~/css/pagination.min.css?v=1.0.9.2" rel="stylesheet" type="text/css" />

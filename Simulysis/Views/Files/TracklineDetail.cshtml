    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/css/bootstrap.min.css" integrity="sha384-zCbKRCUGaJDkqS1kPbPd7TveP5iyJE0EjAuZQTgFLD2ylzuqKfdKlfG/eSrtxUkn" crossorigin="anonymous">*@
</head>
<body>
    <div id="rendered-data">
        <!-- This is where the JavaScript object will be rendered -->
    </div>
    <script>
        window.addEventListener("storage", () => {
                    const data = JSON.parse(window.localStorage.getItem("block"));

        // Create a string to hold the rendered HTML
        let renderedHTML = '';

        // Loop through the properties of the JavaScript object
        for (const key in data) {
            if (data.hasOwnProperty(key)) {
                renderedHTML += `<p>${key}: ${data[key]}</p>`;
            }
        }

        // Update the content of the 'rendered-data' div with the rendered HTML
        document.getElementById("rendered-data").innerHTML = renderedHTML;
        })

    </script>
</body>
</html>
